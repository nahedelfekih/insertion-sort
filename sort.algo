PROCEDURE Insertion_sort(VAR tab : ARRAY_OF INTEGER)
VAR
   i,j,n : INTEGER;
BEGIN
   n := tab.length;
   FOR i FROM 0 TO n- 1 STEP 1  DO
       FOR j  FROM 0 TO n-i-1 STEP 1  DO
           IF (tab[j-1] > tab[j]) THEN
               swap(tab[j], tab[j-1])
           END_IF
       END_FOR
   END_FOR
END